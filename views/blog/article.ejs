  <body class="post-template has-cover">
    <div class="viewport">
      <header id="gh-head" class="gh-head outer">
        <%- blogNavbar %>
      </header>
      <div class="site-content">
        <main id="site-main" class="site-main">
          <article class="article post">
            <header class="article-header gh-canvas">
              <div class="article-tag post-card-tags"></div>
              <h1 class="article-title"><%= title %></h1>
              <p class="article-excerpt">
                <%= summary %>
              </p>
              <div class="article-byline">
                <section class="article-byline-content">
                  <ul class="author-list">
                    <li class="author-list-item">
                      <a href="https://www.multi-brick.com/user/<%= creatorid %>" class="author-avatar">
                        <img
                          class="author-profile-image"
                          src="https://api.multi-brick.com/v1/thumbnails/single?type=1&id=<%= creatorid %>"
                          alt="<%= creatorname %>"
                        />
                      </a>
                    </li>
                  </ul>
                  <div class="article-byline-meta">
                    <h4 class="author-name">
                      <a href="https://www.multi-brick.com/user/<%= creatorid %>"><%= creatorname %></a>
                    </h4>
                    <div class="byline-meta-content">
                      <time class="byline-meta-date" datetime="<%= date %>"
                        ><%= date2 %></time
                      >
                      <span class="byline-reading-time"
                        ><span class="bull">&bull;</span> 5 min read</span
                      >
                    </div>
                  </div>
                </section>
              </div>
              <figure class="article-image">
                <img
                  srcset="
                    <%= image %>  300w,
                    <%= image %>  600w,
                    <%= image %> 1000w,
                    <%= image %> 2000w
                  "
                  sizes="(min-width: 1400px) 1400px, 92vw"
                  src="<%= image %>"
                  alt="<%= title %>"
                />
              </figure>
            </header>
            <section class="gh-content gh-canvas">
              <%- content %>
            </section>
          </article>
        </main>
        <aside class="read-more-wrap outer">
          <div class="read-more inner">
            <%- articles %>
          </div>
        </aside>
      </div>
      <footer class="site-footer outer">
        <div class="inner">
          <section class="copyright">
            <a href="https://blog.multi-brick.com">MultiBrick </a> &copy; <%= year %>
          </section>
          <nav class="site-footer-nav"></nav>
          <!-- <div>
            <a href="https://ghost.org/" target="_blank" rel="noopener"
              >Powered by Ghost</a
            >
          </div> -->
        </div>
      </footer>
